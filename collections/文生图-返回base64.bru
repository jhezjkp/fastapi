meta {
  name: 文生图-返回base64
  type: http
  seq: 6
}

post {
  url: {{baseUrl}}/v1/images/generations
  body: json
  auth: none
}

headers {
  Authorization: Bearer {{apiToken}}
}

body:json {
  {
    "prompt": "a boy, hand draw, line art",
    "n": 1,
    "model": "FLUX.1-schnell",
    "size": "256x256",
    "ResponseFormat": "b64_json"
  }
}

assert {
  res.status: eq 200
  res.body.data[0].b64_json: isString
}
